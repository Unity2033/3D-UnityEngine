		_Matcap("Matcap", 2D) = "white" {}
		_MatcapValue("Matcap Value", Range( -1 , 1)) = 0
		_Smoot("Smoot", Range( 0 , 1)) = 0
		_Metal("Metal", Range( 0 , 1)) = 0
